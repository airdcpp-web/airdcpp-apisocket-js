{
  "name": "airdcpp-apisocket",
  "version": "2.0.0",
  "description": "Javascript connector for AirDC++ Web API",
  "license": "MIT",
  "authors": [
    "maksis"
  ],
  "keywords": [
    "airdcpp",
    "airdcpp-webapi",
    "api",
    "connector",
    "websocket",
    "websocket-api"
  ],
  "bugs": "https://github.com/airdcpp-web/airdcpp-apisocket-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/airdcpp-web/airdcpp-apisocket-js.git"
  },
  "scripts": {
    "build": "npm run build-commonjs && npm run build-es",
    "build-commonjs": "rimraf dist && tsc -d --outDir dist --module CommonJS",
    "build-es": "rimraf dist-es && tsc -d --outDir dist-es --module es2015",
    "clean": "rimraf dist/*",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "events": "^3.0.0",
    "invariant": "^2.2.4",
    "is-in-browser": "^1.1.3",
    "promise": "^8.0.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.9",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^10.12.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-environment-node-debug": "^2.0.0",
    "mock-socket": "~6.0.4",
    "mockdate": "^2.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "websocket": "^1.0.28"
  },
  "main": "./dist/NodeSocket.js",
  "module": "./dist-es/NodeSocket.js"
}
