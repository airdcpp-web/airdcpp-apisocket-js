{
  "name": "airdcpp-apisocket",
  "version": "2.4.4",
  "description": "Javascript connector for AirDC++ Web API",
  "license": "MIT",
  "authors": [
    "maksis"
  ],
  "keywords": [
    "airdcpp",
    "airdcpp-webapi",
    "api",
    "connector",
    "websocket",
    "websocket-api"
  ],
  "bugs": "https://github.com/airdcpp-web/airdcpp-apisocket-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/airdcpp-web/airdcpp-apisocket-js.git"
  },
  "scripts": {
    "build": "npm run build-commonjs && npm run build-es",
    "build-commonjs": "rimraf dist && tsc -d --outDir dist --module CommonJS",
    "build-es": "rimraf dist-es && tsc -d --outDir dist-es --module es2015",
    "clean": "rimraf dist/*",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "chalk": "^5.1.2",
    "events": "^3.3.0",
    "invariant": "^2.2.4",
    "is-in-browser": "^2.0.0",
    "promise": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.11.5",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "jest-environment-node-debug": "^2.0.0",
    "mock-socket": "^9.1.5",
    "pre-commit": "^1.2.2",
    "rimraf": "^4.1.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4",
    "wait-for-expect": "^3.0.2",
    "websocket": "^1.0.34"
  },
  "main": "./dist/NodeSocket.js",
  "module": "./dist-es/NodeSocket.js",
  "type": "module"
}
